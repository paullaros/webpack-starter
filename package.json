{
  "name": "Webpack-Starter",
  "version": "1.1.1",
  "description": "Quickly set-up a webpack based project",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config config/build.config.js --progress --colors",
    "start": "webpack --config config/dev.config.js --watch --progress --colors",
    "serve": "webpack-serve --config config/dev.config.js --content-base dist/ --watch --progress --colors"
  },
  "author": "Paul Laros <paul@laros.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/paullaros/webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/paullaros/webpack-starter/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "8.3.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "node-sass": "4.8.3",
    "postcss-loader": "2.1.4",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.0.1",
    "script-loader": "0.7.2",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-merge": "4.1.2",
    "webpack-serve": "0.3.1"
  },
  "dependencies": {}
}
